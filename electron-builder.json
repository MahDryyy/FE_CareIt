{
    "appId": "com.careit.app",
    "productName": "CareIt",
    "directories": {
        "output": "dist",
        "buildResources": "public"
    },
    "files": [
        "out/**/*",
        "electron.js",
        "preload.js",
        "package.json"
    ],
    "win": {
        "target": "nsis",
        "icon": "public/icon.png"
    },
    "nsis": {
        "oneClick": false,
        "allowToChangeInstallationDirectory": true
    },
    "compression": "normal",
    "electronVersion": "28.1.0"
}